    #proxy_busy_buffers_size 512k; # Must be < (1024k - 256k) = 768k (so 512k is safe)

    # Para poca memoria
    proxy_buffers 4 128k;           # 512k total (en lugar de 1024k)
    proxy_busy_buffers_size 256k;   # Mitad del total

